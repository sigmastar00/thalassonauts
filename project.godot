[application]

config/name="Thalassonauts"
run/main_scene="res://ui/SplashScreen.tscn"
config/icon="res://icon.png"
run/main_scene.editor="res://Main.tscn"

[autoload]

ggsManager="*res://addons/GGS/src/ggs_manager.gd"
Fade="*res://addons/UniversalFade/Fade.gd"
Utils="*res://Utils.gd"

[display]

window/size/width=320
window/size/height=180
window/size/test_width=960
window/size/test_height=540
window/stretch/mode="viewport"
window/stretch/aspect="keep"
window/size/always_on_top.debug=false

[editor_plugins]

enabled=PoolStringArray( "res://addons/AsepriteWizard/plugin.cfg", "res://addons/GGS/plugin.cfg", "res://addons/jamcountdown/plugin.cfg" )

[gdnative]

singletons=[ "res://addons/godot-git-plugin/git_api.gdnlib" ]

[importer_defaults]

texture={
"flags/filter": false
}

